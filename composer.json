{
    "name": "amp/pdfcrowd-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony2 bundle for working with the PDFCrowd API",
    "keywords": ["pdfcrowd", "pdf"],
    "homepage": "https://github.com/hubertperron/AmpPDFCrowdBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Hubert Perron",
            "email": "hubert.perron@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "pdfcrowd/pdfcrowd-php",
                "version": "2.6",
                "dist": {
                    "url": "http://pdfcrowd.com/static/clients/php/pdfcrowd-2.6-php.zip",
                    "type": "zip"
                },
                "autoload": {
                    "files": ["pdfcrowd.php"]
                }
            }
        }
    ],
    "require": {
        "pdfcrowd/pdfcrowd-php": "2.6",
        "symfony/symfony": "2.*"
    },
    "autoload": {
        "psr-0": { "Amp\\PDFCrowdBundle": "" }
    },
    "target-dir": "Amp/PDFCrowdBundle"
}
